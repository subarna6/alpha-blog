<h1>LOG IN</h1>
<%= form_for :session, url: login_path do |f| %>
<p>
 <%= f.label :email %> <br/>
 <%= f.email_field :email %>
</p>
<p>
  <%= f.label :password %> <br/>
  <%= f.password_field :password %>
</p>
<p>
  <%= f.submit %>
</p>
<% end %>

<%= link_to 'Back to article listing.', articles_path %>